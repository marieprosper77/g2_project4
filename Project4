CREATE TABLE credit_record2 (
  ID VARCHAR,
  MONTHS_BALANCE DECIMAL,
  STATUS VARCHAR
)

SELECT * FROM credit_record2
WHERE id = 5001711

CREATE TABLE application_record (
	ID VARCHAR PRIMARY KEY,
	CODE_GENDER VARCHAR,
	FLAG_OWN_CAR VARCHAR, 
	FLAG_OWN_REALTY VARCHAR,
	CNT_CHILDREN INTEGER,
	AMT_INCOME_TOTAL FLOAT,
	NAME_INCOME_TYPE VARCHAR,
	NAME_EDUCATION_TYPE VARCHAR, 
	NAME_FAMILY_STATUS VARCHAR, 
	NAME_HOUSING_TYPE VARCHAR,
	DAYS_BIRTH INTEGER, 
	DAYS_EMPLOYED INTEGER,
	FLAG_MOBIL INTEGER, 
	FLAG_WORK_PHONE INTEGER, 
	FLAG_PHONE INTEGER, 
	FLAG_EMAIL INTEGER, 
	OCCUPATION_TYPE VARCHAR,
	CNT_FAM_MEMBERS DECIMAL
)

SELECT * FROM application_record; 


SELECT * FROM application_record A
INNER JOIN credit_record2 C
ON A.id = C.ID,


